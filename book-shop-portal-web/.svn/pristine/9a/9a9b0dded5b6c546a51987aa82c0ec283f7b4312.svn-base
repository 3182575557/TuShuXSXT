.combobox-book,
.combobox-group {
  font-size: 12px;
  padding: 3px;
  padding-right: 0px;
}
.combobox-book-disabled {
  opacity: 0.5;
  filter: alpha(opacity=50);
}
.combobox-gbook {
  padding-left: 10px;
}
.combobox-group {
  font-weight: bold;
}
.combobox-book-hover {
  background-color: #e2e2e2;
  color: #000000;
}
.combobox-book-selected {
  background-color: #0092DC;
  color: #fff;
}
